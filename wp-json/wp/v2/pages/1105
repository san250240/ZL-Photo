r\n    \/\/ \u2705 Phone number validation\r\n    const phone = document.getElementById(\"phone\").value.replace(\/\\D\/g, \"\");\r\n    if (phone.length !== 10) {\r\n      messageDiv.textContent = 'Please enter a valid 10-digit phone number.';\r\n      messageDiv.classList.add('error');\r\n      const phoneField = document.getElementById(\"phone\");\r\n      phoneField.focus({preventScroll:true});\r\n      phoneField.scrollIntoView({behavior: 'smooth', block: 'center'});\r\n      return;\r\n    }\r\n\r\n    if (!form.checkValidity()) {\r\n      const firstInvalid = form.querySelector(':invalid');\r\n      if (firstInvalid) {\r\n        firstInvalid.focus({preventScroll:true});\r\n        firstInvalid.scrollIntoView({behavior: 'smooth', block: 'center'});\r\n      }\r\n      messageDiv.textContent = 'Please fill in all required fields correctly.';\r\n      messageDiv.classList.add('error');\r\n      return;\r\n    }\r\n\r\n    const submitButton = form.querySelector('button[type=\"submit\"]');\r\n    submitButton.disabled = true;\r\n\r\n    \/\/ Create FormData and remove empty fields\r\n    const formData = new FormData(form);\r\n    for (let [key, value] of formData.entries()) {\r\n      if (!value.trim()) {\r\n        formData.delete(key);\r\n      }\r\n    }\r\n\r\n    try {\r\n      const response = await fetch('https:\/\/api.web3forms.com\/submit', {\r\n        method: 'POST',\r\n        body: formData,\r\n      });\r\n\r\n      const data = await response.json();\r\n\r\n      if (data.success) {\r\n        form.reset();\r\n        messageDiv.textContent = \"Thank you for reaching out! You should receive a confirmation email shortly. If it doesn\u2019t arrive, please send another message through the contact form or write me directly via my email address at the top of this page.\";\r\n        messageDiv.classList.add('success');\r\n      } else {\r\n        messageDiv.textContent = data.message || 'Oops! There was a problem submitting your form. Please send another message through the contact form or reach out directly via my email address at the top of this page.';\r\n        messageDiv.classList.add('error');\r\n      }\r\n    } catch (error) {\r\n      messageDiv.textContent = 'Oops! There was a problem submitting your form. Please send another message through the contact form or reach out directly via my email address at the top of this page.';\r\n      messageDiv.classList.add('error');\r\n    } finally {\r\n      submitButton.disabled = false;\r\n    }\r\n  });\r\n<\/script>\r\n\r\n<\/body>\r\n<\/html>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Contact Serving MassachusettsNew England Email Address samliangphoto@gmail.com Phone Number 617-249-4059 Let&#8217;s Chat Can&#039;t wait to hear from you! | Thank you so much for your interest in having me as your wedding photographer! Once you submit this form, I\u2019ll get back to you with my availability for your date. I limit the number of weddings I take on per year in order to give my clients the best experience possible. From there, we\u2019ll set up a time to connect (in person or virtually), so we can get to know each other and chat about your wedding plans. Can&#8217;twaittohearfromyou! Contact Form Names* Pronouns for you both Phone number* Email* What date are you inquiring about? What type of session are you interested in?* Choose oneElopementEngagement SessionWeddingOther Estimated guest count How did you find out about me? More details Submit<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"","meta":{"footnotes":""},"class_list":["post-1105","page","type-page","status-publish","hentry"],"blocksy_meta":{"page_title_panel":"","has_hero_section":"disabled","572c47b14932e2f0aa39fe8a3b02ea1c":"","hero_section":"type-1","hero_e